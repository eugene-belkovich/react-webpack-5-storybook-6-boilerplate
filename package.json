{
  "name": "react-webpack-5-storybook-6-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate for quick start of new project with webpack 5 and storybook 6",
  "engines": {
    "node": ">= 14.17.4",
    "npm": ">= 6.14.15"
  },
  "scripts": {
    "start:dev": "npm run clean && cross-env NODE_ENV=development webpack serve --progress --color",
    "start:prod": "npm run build:prod && npm run start-prod-server",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --progress --color",
    "start-prod-server": "cross-env NODE_ENV=production node server.js",
    "build": "webpack --mode production",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint \"**/*.js\" --ignore-path .gitignore --ignore-pattern internals -- . ",
    "lint:js:fix": "eslint \"**/*.js\" --ignore-path .gitignore --ignore-pattern internals --fix -- .",
    "refactor": "npm run prettify && npm run lint:js:fix",
    "prettify": "prettier --write \"**/*.js\" --ignore-path .eslintignore",
    "test": "cross-env NODE_ENV=test jest --no-cache",
    "test:changed": "jest -o",
    "test:build": "npm run lint && jest",
    "test:clean": "rimraf ./coverage",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:update": "npm run refactor && cross-env NODE_ENV=test jest -u",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "clean": "rimraf ./docs"
  },
  "author": "Eugene Belkovich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eugene-belkovich/react-webpack-5-storybook-6-boilerplate/issues"
  },
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run refactor",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "@material-ui/core": "^4.12.3",
    "@mui/material": "^5.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/register": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "@loadable/babel-plugin": "^5.13.2",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/lodash": "^4.14.165",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "autoprefixer": "^10.1.0",
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "coveralls": "^3.1.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "figlet": "^1.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.5",
    "json-loader": "^0.5.7",
    "loadable-ts-transformer": "^1.0.0-alpha.3",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "react-refresh": "^0.9.0",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/builder-webpack5": "^6.3.7",
    "@storybook/manager-webpack5": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
